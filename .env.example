# Exemple de fichier d'environnement
# Copier en .env et compléter avec vos valeurs

# Configuration générale
DEBUG=False
SEED=42
DEVICE=cuda  # ou 'cpu'

# Logging
LOG_LEVEL=INFO
LOG_DIR=logs

# Données financières
FRED_API_KEY=your_fred_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
QUANDL_API_KEY=your_quandl_api_key_here

# Weights & Biases
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=capital-structure-rl
WANDB_ENTITY=your_entity_here

# Training
CUDA_VISIBLE_DEVICES=0
NUM_WORKERS=4

# Modèles
MODEL_SAVE_PATH=models
BEST_MODEL_PATH=models/best_models
